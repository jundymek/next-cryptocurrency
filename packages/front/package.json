{
  "name": "front",
  "version": "0.1.0",
  "main": "index.js",
  "engines": {
    "node": "15.10.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start-dev": "next start",
    "start": "next start -p $PORT",
    "publish-heroku": "cd ../../ && git subtree push --prefix packages/front next-crypto-front master"
  },
  "dependencies": {
    "@iconify/icons-ic": "^1.1.3",
    "@iconify/icons-mdi": "^1.1.12",
    "autoprefixer": "^10.1.0",
    "clsx": "^1.1.1",
    "coinmarketcap-cryptocurrency-icons": "^1.0.1",
    "next": "10.0.4",
    "postcss": "^8.2.2",
    "postcss-import": "^14.0.0",
    "react": "^16.8.0",
    "react-confirm-alert": "^2.7.0",
    "react-dom": "^16.8.0",
    "react-multi-carousel": "^2.6.2",
    "react-select": "^4.1.0",
    "sass": "^1.32.6",
    "sharp": "^0.27.2",
    "styled-components": "^5.2.1",
    "swr": "^0.4.0",
    "tailwindcss": "^2.0.2",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@iconify/icons-codicon": "^1.1.11",
    "@iconify/icons-et": "^1.1.0",
    "@iconify/react": "^1.1.4",
    "@types/node": "^14.14.33",
    "@types/react-select": "^4.0.13",
    "@types/styled-components": "^5.1.7",
    "typescript": "^4.1.3"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": false
            }
          ]
        ]
      }
    }
  }
}
